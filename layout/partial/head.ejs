<head>
    <meta charset='UTF-8'>
    <meta name='viewport' content='width=device-width, initial-scale=1.0'>
    <meta http-equiv='X-UA-Compatible' content='ie=edge'>
    <meta name='description' content='<%- page.synopsis || __('description') %>'>
    <title>
        <%- __('title') %>

        <%if (page.title != null && page.title.length > 0) { %>
            - <%- page.title %>
        <% } %>

        <%if (page.part) { %>
            - <%- __('part') %> <%- page.part %>
        <% } %>
    </title>

    <link rel='stylesheet' href='<%- url_for('/layout.css') %>'>
    <link rel='preload' href='<%- url_for('/icomoon/fonts.ttf?a3zlv3') %>' as='font' type='font/ttf' crossorigin='anonymous'>
    <link rel='apple-touch-icon' sizes='180x180' href='<%- url_for('/img/apple-touch-icon.png') %>'>
    <link rel='icon' type='image/png' sizes='32x32' href='<%- url_for('/img/favicon-32x32.png') %>'>
    <link rel='icon' type='image/png' sizes='16x16' href='<%- url_for('/img/favicon-16x16.png') %>'>
    <link rel='manifest' href='<%- url_for('/site.webmanifest') %>'>

    <style>
        :root {
            --color-primary: <%=theme.primaryColor %>;
        }
    </style>
</head>

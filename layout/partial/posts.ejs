<div class='<%- !theme.wikiMode ? "blog-posts" : "wiki-posts" %>'>
    <% posts.filter(post => post.indexable != false).each(function(post, i) { %>

        <% if(!theme.wikiMode) { %>
            <%- partial('partial/blogPost', { post: post }) %>
        <% } else { %>
            <%- partial('partial/wikiPost', { post: post }) %>
        <% } %>

    <% }) %>
</div>
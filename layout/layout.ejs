<!DOCTYPE html>
<html lang='<%- page.lang %>'>

<%- partial('partial/head') %>

<body>
    <span id='top'></span>

    <% if(!is_home()) { %>
    <a href='<%- url_for_lang('/') %>' class='home-button'>
        <i class='icon-home'></i>
    </a>
    <% } %>

    <%- partial('partial/menu') %>

    <%- body %>

    <footer>
        <p><%- __('footerText') %></p>

        <% if(!config.notShowRepo && config.repo != null) { %>
        <p><%= __('curiosityOfHowWasMade') %> <br> <a href='<%- url_for(config.repo) %>' target='_blank'
                rel='noreferrer'><%= __('accessRepo') %></a></p>
        <% } %>

        <a href='#top' class='back-to-top'>
            <%= __('backToTop') %> <i class='icon-arrow'></i>
        </a>
    </footer>
</body>

</html>